{
  "name": "injuryprofiler-calendar-prototype",
  "version": "1.0.0",
  "private": true,
  "description": "Pixel-perfect replica of injuryprofiler.com/calendar with mock data",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "stylelint 'src/**/*.css' --fix",
    "validate-design-system": "node scripts/validate-design-system.js",
    "validate-quick": "npm run lint && npm run lint:css"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/list": "^6.1.8",
    "@fullcalendar/moment-timezone": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@mui/x-data-grid": "^8.13.1",
    "@mui/x-date-pickers": "^8.11.3",
    "date-fns": "^4.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-design-system": "file:./eslint-plugin-design-system",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "stylelint": "^15.0.0",
    "stylelint-config-standard": "^30.0.0",
    "stylelint-plugin-design-system": "file:./stylelint-plugin-design-system",
    "vite": "^4.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate-design-system"
    }
  }
}
